#!/usr/bin/python

import sys

for line in sys.stdin:
    line = line.strip().split()
    family = line[3]
    score = int(line[4])
    if line[12] != '.':
        v = line[12].split(',')
        u = []
        i = 0
        while i < len(v):
            new_family = v[i].split(':')[0]
            new_score = int(v[i].split(':')[1])
            if new_score == score:
                family += ',' + new_family
            else:
                u.append(v[i])
            i += 1
        if len(u) > 0:
            line[12] = ','.join(u)
        else:
            line[12] = '.'
    line[3] = family
    print '\t'.join(line)
